using System.Runtime.Intrinsics;
using System.Runtime.Intrinsics.X86;

public class C
{
    public int M(Vector256<int> vector) {
        var add1 = Sse2.Add(vector.GetLower(), vector.GetUpper());
        return add1.ToScalar();
    }
}

