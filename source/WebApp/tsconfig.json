{
    "compilerOptions": {
        "target": "es2019",
        "moduleResolution": "node",
        "noEmit": true,
        "strict": true,
        "importsNotUsedAsValues": "error",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules/**/*",
        "build.js",
        "wwwroot/**/*"
    ]
}